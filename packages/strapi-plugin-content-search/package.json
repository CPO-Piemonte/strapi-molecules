{
  "name": "strapi-plugin-content-search",
  "version": "0.3.2",
  "description": "Strapi search plugin provides /search endpoint that can be used to find content type fields which contain search string",
  "strapi": {
    "name": "content-search",
    "icon": "plug",
    "description": "Search api for searchable models"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "clean": "rimraf dist coverage",
    "clean-all": "rimraf dist coverage node_modules",
    "extract-dist-to-root": "ts-node ../../scripts/extract-dist-to-root.ts",
    "clean-dist-from-root": "ts-node ../../scripts/clean-dist-from-root.ts",
    "prepublishOnly": "yarn build && yarn extract-dist-to-root",
    "postpublish": "yarn clean-dist-from-root"
  },
  "dependencies": {
    "strapi-deepsearch-service": "0.3.1"
  },
