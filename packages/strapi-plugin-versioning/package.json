{
  "name": "strapi-plugin-versioning",
  "version": "0.1.0",
  "description": "Strapi plugin for collecting versions of content type entities",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "clean": "rimraf dist coverage",
    "clean-all": "rimraf dist coverage node_modules",
    "set-development-package-json": "ts-node ../../scripts/set-development-package-json.ts",
    "set-publish-package-json": "ts-node ../../scripts/set-publish-package-json.ts",
    "extract-dist-to-root": "ts-node ../../scripts/extract-dist-to-root.ts",
    "clean-dist-from-root": "ts-node ../../scripts/clean-dist-from-root.ts",
    "prepublishOnly": "yarn build && yarn set-publish-package-json",
    "postpublish": "yarn set-development-package-json",
    "postinstall": "cp -r dist/extension-file-to-copy/content-manager $INIT_CWD/extensions"
  },
  "dependencies": {
    "lodash": "^4.17.12",
    "strapi-utils": "^3.0.6"
  },
  "author": {
    "name": "VirtusLab",
    "email": "strapi@virtuslab.com",
    "url": "https://virtuslab.com"
  },
  "maintainers": [
    {
      "name": "VirtusLab",
      "email": "strapi@virtuslab.com",
      "url": "https://virtuslab.com"
    }
  ],
  "homepage": "https://github.com/VirtusLab/strapi-molecules",
  "repository": {
    "type": "git",
    "url": "https://github.com/VirtusLab/strapi-molecules"
  },
  "bugs": {
    "url": "https://github.com/VirtusLab/strapi-molecules/issues",
    "email": "strapi@virtuslab.com"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "strapi",
    "utilities",
    "utils",
    "versions",
    "versioning"
  ]
}
